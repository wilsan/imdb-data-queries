<!DOCTYPE html>
<html lang="en">

<head>
   <meta charset="UTF-8">
   <meta http-equiv="X-UA-Compatible" content="IE=edge">
   <meta name="viewport" content="width=device-width, initial-scale=1.0">
   <title>Query #1</title>
   <link rel="stylesheet" href="stylesheets/home.css">
</head>

<body>
   <h3>Query Result:</h3>
   <table>
      <tr>
         <% fields.forEach((item) => { %>
         <th><%= item %></th>
         <% }) %>
      </tr>

      <% for (let movie of movies) { %>
      <tr>
         <% fields.forEach((item) => { %>
         <td><%= movie[item] %></td>
         <% }) %>
      </tr>
      <% } %>
   </table>
</body>

</html>